/*#include <iostream>
#include <string>
using namespace std;
int fib(int num){
    if((num==0)||(num==1)){
        return num;
    }
    else{
        return (fib(num-1)+fib(num-2));
    }
}
int main(){
    int n,i,num;
    cin>>n;
    for(i=0;i<n;i++){
        cin>>num;
        fib(num);
        cout<<fib(num)<<endl;
    }
return 0;
}*/
