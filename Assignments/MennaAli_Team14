//(D)
/*#include <iostream>
using namespace std;
int i,j,temp;
void sorting(long arr[],long n){
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i;j++){
            if(arr[j]>arr[j+1]){
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
}
int main()
{
    long n;
    cin>>n;
    long arr[n];
    for(i=0;i<n;i++){
        cin>>arr[i];
    }
    sorting(arr,n);
    for(i=0;i<n;i++){
        cout<<arr[i];
    }
    return 0;
}*/
