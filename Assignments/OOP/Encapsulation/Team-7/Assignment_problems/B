#include <iostream>

using namespace std;

void Reverse(string&);

int main()
{
    int t;
    string x;

    cin >> t;

    for (size_t i = 0; i < t; i++)
    {
        cin >> x;
        Reverse(x);
        cout << x << endl;
    }
   return(0);
}

void Reverse(string &x)
{
    int len = x.size(), temp;
    for (size_t i = 0, j = 1; i < len/2; i++,j++)
    {
        temp = x[i];
        x[i] = x[len-j];
        x[len-j] = temp;
    }
}
